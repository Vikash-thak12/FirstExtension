{
    "manifest_version": 3,
    "name": "Vikash",
    "description": "My Firest Chrome Extension",
    "version": "1.0",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
  }